@prefix : <http://www.semanticweb.org/YOUTH/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/YOUTH/> .

<http://www.semanticweb.org/YOUTH> rdf:type owl:Ontology ;
                                    owl:imports <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl> .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/YOUTH/affectsPerspective
:affectsPerspective rdf:type owl:ObjectProperty ,
                             owl:TransitiveProperty ;
                    rdfs:domain :PerspectiveChange ;
                    rdfs:range :ExternalPerspective ,
                               :InternalPerspective ;
                    rdfs:comment "This property links a perspective change to the specific external or internal perspectives it affects. It captures how shifts in perspective affect the views of subcultures both internally (self-perception) and externally (public perception)." .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :affectsPerspective ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "This property links a perspective change to the specific external or internal perspectives it affects. It captures how shifts in perspective affect the views of subcultures both internally (self-perception) and externally (public perception)." ;
   rdfs:comment "Transitive property: if a change in perspective influences another, and that one influences a third, then the first one indirectly influences the third."
 ] .


###  http://www.semanticweb.org/YOUTH/engagesIn
:engagesIn rdf:type owl:ObjectProperty ,
                    owl:AsymmetricProperty ;
           rdfs:domain :Person ;
           rdfs:range :Ritual ;
           rdfs:comment "Links individuals to rituals and activities they typically engage in." .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :engagesIn ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Links individuals to rituals and activities they typically engage in." ;
   rdfs:comment "Asymmetric: engaging in a ritual does not imply the ritual engages in the person."
 ] .


###  http://www.semanticweb.org/YOUTH/hasFashionStyle
:hasFashionStyle rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :relatedTo ;
                 rdfs:domain :YouthSubculture ;
                 rdfs:range :FashionStyle ;
                 rdfs:comment "Links a subculture to its fashion style(s)." .


###  http://www.semanticweb.org/YOUTH/hasMusicGenre
:hasMusicGenre rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :relatedTo ;
               rdfs:domain :YouthSubculture ;
               rdfs:range :MusicGenre ;
               rdfs:comment "Links a subculture to its associated music genre(s)." .


###  http://www.semanticweb.org/YOUTH/hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                owl:inverseOf :participatesIn ;
                rdfs:domain :YouthSubculture ;
                rdfs:range :Person ;
                rdfs:comment "Links a youth subculture to a person (indicating that the person is a member of the subculture)." .


###  http://www.semanticweb.org/YOUTH/hasValue
:hasValue rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :relatedTo ;
          rdfs:domain :YouthSubculture ;
          rdfs:range :Value ;
          owl:propertyDisjointWith :triggersStereotype ;
          rdfs:comment "Links a subculture to its core values." .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :hasValue ;
   owl:annotatedProperty owl:propertyDisjointWith ;
   owl:annotatedTarget :triggersStereotype ;
   rdfs:comment "Values and stereotypes have different implications."
 ] .


###  http://www.semanticweb.org/YOUTH/influencedBy
:influencedBy rdf:type owl:ObjectProperty ;
              rdfs:domain :ExternalPerspective ,
                          :InternalPerspective ;
              rdfs:range :PerspectiveInfluence ;
              rdfs:comment "Connects perspectives to the factors that influence them. This property shows how external and internal perspectives are affected by various influencing factors such as media representations, societal attitudes, and personal experiences. It models the relationship between perspectives and the elements that shape them." .


###  http://www.semanticweb.org/YOUTH/influences
:influences rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :YouthSubculture ;
            rdfs:range :Person ,
                       :YouthSubculture ;
            owl:propertyDisjointWith :triggersStereotype ;
            rdfs:comment "Represents how a subculture influences the perspectives of individuals or other subcultures. This property models the impact that a subculture has on shaping the views and self-concepts of its members, as well as its influence on other subcultures. It captures the reciprocal effects and interactions between subcultures and their members or peers." .


###  http://www.semanticweb.org/YOUTH/locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :YouthSubculture ;
           rdfs:range :Location ;
           rdfs:comment "Links individuals and subcultures to geographic locations." .


###  http://www.semanticweb.org/YOUTH/originatedIn
:originatedIn rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :YouthSubculture ;
              rdfs:range :HistoricalPeriod ;
              rdfs:comment "Connects the youth subculture to its historical period, representing the time frame during which they have developed or undergone significant changes." .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :originatedIn ;
   owl:annotatedProperty rdfs:comment ;
   owl:annotatedTarget "Connects the youth subculture to its historical period, representing the time frame during which they have developed or undergone significant changes." ;
   rdfs:comment "A subculture typically originates in one historical period, so this is a functional property."
 ] .


###  http://www.semanticweb.org/YOUTH/participatesIn
:participatesIn rdf:type owl:ObjectProperty ;
                rdfs:domain :Person ;
                rdfs:range :YouthSubculture ;
                rdfs:comment "Links a Person to a YouthSubculture (indicating that the person belongs to that subculture)." .


###  http://www.semanticweb.org/YOUTH/relatedTo
:relatedTo rdf:type owl:ObjectProperty ;
           rdfs:domain :YouthSubculture ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Media
                                      :Ritual
                                      :Style
                                    )
                      ] ;
           rdfs:comment """General property for linking various elements. 
It links subcultures to relevant media, rituals or styles.""" .


###  http://www.semanticweb.org/YOUTH/relatedToExpressionMedia
:relatedToExpressionMedia rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :relatedTo ;
                          rdfs:domain :YouthSubculture ;
                          rdfs:range :ExpressionMedia ;
                          rdfs:comment "Links subcultures to forms of expression media." .


###  http://www.semanticweb.org/YOUTH/relatedToMainstreamMedia
:relatedToMainstreamMedia rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :relatedTo ;
                          rdfs:range :MainstreamMedia ;
                          rdfs:comment "Links subcultures to mainstream media representations." .


###  http://www.semanticweb.org/YOUTH/selfViewsAs
:selfViewsAs rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :affectsPerspective ;
             rdfs:domain :Person ;
             rdfs:range :InternalPerspective ;
             rdfs:comment "Links individuals to their internal viewpoint within a youth subculture. This property represents how members of a subculture perceive themselves and their identity, capturing personal self-views and self-descriptions that reflect their own understanding and experience within the subculture." .


###  http://www.semanticweb.org/YOUTH/triggersStereotype
:triggersStereotype rdf:type owl:ObjectProperty ;
                    rdfs:domain :YouthSubculture ;
                    rdfs:range :Stereotype ;
                    rdfs:comment "Links a stereotype to the subcultures that it affects or challenges. This property identifies how certain stereotypes are associated with subcultures and how these stereotypes may influence or be related to the subculture’s public image. It highlights the stereotypes that subcultures confront or embody." .


###  http://www.semanticweb.org/YOUTH/viewedAs
:viewedAs rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :affectsPerspective ;
          rdfs:domain :YouthSubculture ;
          rdfs:range :ExternalPerspective ;
          rdfs:comment "Links a youth subculture to its external viewpoint. This property connects subcultures to the way they are perceived by outsiders. It helps to model the public image and external characterization of a subculture." .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/YOUTH/hasContext
:hasContext rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :HistoricalPeriod ;
            rdfs:range rdfs:Literal ;
            rdfs:comment "Describes the historical background in which a youth subculture originated." .


###  http://www.semanticweb.org/YOUTH/hasName
:hasName rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Person ;
         rdfs:range rdfs:Literal ;
         rdfs:comment "Indicates the name of a person." .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/YOUTH/ExpressionMedia
:ExpressionMedia rdf:type owl:Class ;
                 rdfs:subClassOf :Media ;
                 rdfs:comment "Types of media or specific media forms consumed by the subculture (e.g., Rock Biographies, Punk Zine). This column indicates the media formats frequently used by the subculture." .


###  http://www.semanticweb.org/YOUTH/ExternalPerspective
:ExternalPerspective rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Lens> ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :influencedBy ;
                                       owl:someValuesFrom :PerspectiveInfluence
                                     ] ;
                     owl:disjointWith :InternalPerspective ;
                     rdfs:comment "Represents the way in which subcultures are viewed from an external viewpoint. This is influenced by the general public's impressions, stereotypes, and media portrayals that shape the external image of a subculture." .


###  http://www.semanticweb.org/YOUTH/FashionStyle
:FashionStyle rdf:type owl:Class ;
              rdfs:subClassOf :Style ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasFashionStyle ;
                                owl:someValuesFrom :Style
                              ] ;
              owl:disjointWith :MusicGenre ;
              rdfs:comment "Represents the style of clothing and aesthetics associated with a subculture." .


###  http://www.semanticweb.org/YOUTH/HistoricalPeriod
:HistoricalPeriod rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation> ;
                  owl:disjointWith :Location ;
                  rdfs:comment "Represents the time frame (time-span) during which a subculture emerged, developed, or experienced significant influences." .


###  http://www.semanticweb.org/YOUTH/InternalPerspective
:InternalPerspective rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Lens> ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :influencedBy ;
                                       owl:someValuesFrom :PerspectiveInfluence
                                     ] ;
                     rdfs:comment "Represents the self-perception and self-definition of members within a subculture. This includes how individuals within the subculture view themselves and their own identity, as well as how they interpret their place within the subculture. It reflects the internal self-concept and personal narratives that members use to describe their experiences and values." .


###  http://www.semanticweb.org/YOUTH/Location
:Location rdf:type owl:Class ;
          rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation> ;
          rdfs:comment "Represents the geographic context or physical setting where a subculture originates, thrives, or is predominantly practiced. This includes specific regions, cities, or areas that are significant to the subculture's identity and activities." .


###  http://www.semanticweb.org/YOUTH/MainstreamMedia
:MainstreamMedia rdf:type owl:Class ;
                 rdfs:subClassOf :Media ;
                 rdfs:comment "Represents mainstream media forms related to subcultures (e.g. TV programs, ducumentaries, etc.)" .


###  http://www.semanticweb.org/YOUTH/Media
:Media rdf:type owl:Class ;
       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Background> .


###  http://www.semanticweb.org/YOUTH/MusicGenre
:MusicGenre rdf:type owl:Class ;
            rdfs:subClassOf :Style ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasMusicGenre ;
                              owl:someValuesFrom :Style
                            ] ;
            rdfs:comment "Represents the type of music associated with a subculture." .


###  http://www.semanticweb.org/YOUTH/Person
:Person rdf:type owl:Class ;
        owl:equivalentClass _:genid13 ;
        rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :influencedBy ;
                          owl:someValuesFrom :ExternalPerspective
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :influencedBy ;
                          owl:someValuesFrom :InternalPerspective
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :influencedBy ;
                          owl:someValuesFrom :Stereotype
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :participatesIn ;
                          owl:someValuesFrom :YouthSubculture
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :selfViewsAs ;
                          owl:someValuesFrom :InternalPerspective
                        ] ;
        owl:hasKey ( :hasName
                   ) ;
        rdfs:comment "Individuals within the subcultures." .

_:genid13 owl:intersectionOf ( :Person
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :participatesIn ;
                                  owl:someValuesFrom :YouthSubculture
                                ]
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :selfViewsAs ;
                                  owl:someValuesFrom :InternalPerspective
                                ]
                              ) ;
           rdf:type owl:Class .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Person ;
   owl:annotatedProperty owl:equivalentClass ;
   owl:annotatedTarget _:genid13 ;
   rdfs:comment "A chain that links a Person’s internal perspective to a YouthSubculture through participation."
 ] .


###  http://www.semanticweb.org/YOUTH/PerspectiveChange
:PerspectiveChange rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Cut> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :affectsPerspective ;
                                     owl:someValuesFrom [ rdf:type owl:Class ;
                                                          owl:unionOf ( :ExternalPerspective
                                                                        :InternalPerspective
                                                                      )
                                                        ]
                                   ] ;
                   owl:disjointWith :PerspectiveInfluence ;
                   rdfs:comment "Represents the evolution or transformation of perspectives over time. This class captures shifts in how subcultures are viewed, whether by the public or by their own members. It includes changes in societal attitudes, evolving stereotypes, or shifts in self-perception within the subculture, highlighting how perspectives can develop and alter across different historical or cultural contexts." .


###  http://www.semanticweb.org/YOUTH/PerspectiveInfluence
:PerspectiveInfluence rdf:type owl:Class ;
                      rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Background> ;
                      rdfs:comment """Represents the factors that shape and affect both external and internal perspectives. This can include a variety of influences such as media portrayals, social institutions - like family, work, school, home -, personal experiences, and cultural trends, all of which contribute to the evolving perceptions of subcultures by both insiders and outsiders. Hall’s Encoding/Decoding theory is integrated into this analysis, allowing us to deconstruct the role of television as a primary source of information on the subject.

See bibliography:
Hall, Stuart. \"Encoding/Decoding in the Television Discourse.\" University of Birmingham: Centre for Contemporary Cultural Studies, 1973.
Moore, Ryan. \"Alternative to What? Subcultural Capital and the Commercialization of a Music Scene.\" Deviant Behavior 26, no. 3 (2005): 229-252.""" .


###  http://www.semanticweb.org/YOUTH/Ritual
:Ritual rdf:type owl:Class ;
        rdfs:subClassOf :Style ;
        owl:disjointWith :Value ;
        rdfs:comment "Represents the activities that usually characterize a specific youth subculture." .


###  http://www.semanticweb.org/YOUTH/Stereotype
:Stereotype rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :Stereotype
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :triggersStereotype ;
                                                         owl:someValuesFrom :YouthSubculture
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Attitude> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :triggersStereotype ;
                              owl:someValuesFrom :YouthSubculture
                            ] ;
            owl:disjointWith :Value ;
            rdfs:comment "Represents common stereotypes associated with subcultures. This class encompasses generalized beliefs and oversimplified notions that are commonly attributed to members of specific subcultures. These stereotypes often reflect societal perceptions and can influence how subcultures are understood and portrayed in mainstream discourse. By capturing stereotypes, this class helps to highlight and analyze the misrepresentations and biases that may affect the public's view of various subcultures, offering insights into how these groups are perceived versus their actual characteristics and values." .


###  http://www.semanticweb.org/YOUTH/Style
:Style rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :relatedTo ;
                         owl:someValuesFrom [ rdf:type owl:Class ;
                                              owl:unionOf ( :FashionStyle
                                                            :MusicGenre
                                                          )
                                            ]
                       ] ;
       owl:disjointWith :YouthSubculture ;
       rdfs:comment "Represents the overall style associated with a subculture, which can be further divided into Fashion Style, Music Genre, Ritual." .


###  http://www.semanticweb.org/YOUTH/Value
:Value rdf:type owl:Class ;
       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Attitude> ;
       rdfs:comment "Represents the fundamental principles and beliefs that underpin a subculture, guiding the behavior, attitudes, and norms of its members." .


###  http://www.semanticweb.org/YOUTH/YouthSubculture
:YouthSubculture rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                              owl:onProperty :engagesIn ;
                                                              owl:someValuesFrom :Ritual
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasFashionStyle ;
                                                              owl:someValuesFrom :FashionStyle
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasMusicGenre ;
                                                              owl:someValuesFrom :MusicGenre
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasParticipant ;
                                                              owl:someValuesFrom :Person
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasValue ;
                                                              owl:someValuesFrom :Value
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Eventuality> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :locatedIn ;
                                   owl:someValuesFrom :Location
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :originatedIn ;
                                   owl:someValuesFrom :HistoricalPeriod
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :relatedTo ;
                                   owl:someValuesFrom :Style
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :viewedAs ;
                                   owl:someValuesFrom :ExternalPerspective
                                 ] ,
                                 _:genid54 ;
                 <http://purl.org/dc/elements/1.1/creator> "Lucrezia Pograri" ,
                                                           "Pietro Tisci" ;
                 <http://purl.org/dc/elements/1.1/date> 2024 ;
                 rdfs:comment """A group of people within a cultural society that represents a specific subculture formed by younger generations, characterized by distinct values, practices, and identities that differentiate them from the dominant culture.

See bibliography:
Cohen, Albert K. Delinquent Boys: The Culture of the Gang. New York: Free Press, 1955.
Cohen, Phil. \"Subcultural Conflict and Working-Class Community.\" In CCCS Selected Working Papers, edited by Ann Gray, Jan Campbell, and Mark Erickson, 554-577. London: Routledge, 2007.
Williams, Patrick J. \"Youth Subcultural Studies: Sociological Traditions and Core Concepts.\" Sociology Compass 1, no. 2 (2007): 572-593.
Widdicombe, Sue. \"Autobiography and Change: Rhetoric and Authenticity of 'Gothic' Style.\" In Repertoires and Readings of Texts in Action, edited by Erica Burman and Ian Parker, 94-113. London: Routledge, 1993.""" ;
                 rdfs:seeAlso "https://krke24.gitbook.io/krke-project-2023-2024" .

_:genid54 rdf:type owl:Restriction ;
           owl:onProperty :influences ;
           owl:hasSelf "true"^^xsd:boolean .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :YouthSubculture ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget _:genid54 ;
   rdfs:comment "A youth subculture could potentially influence itself, but it's not necessarily always the case. Hence, the \"influences\" property is \"potentially reflexive\"."
 ] .


