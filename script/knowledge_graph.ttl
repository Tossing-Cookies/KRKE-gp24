        youth:risk-taking,
        youth:troublemakers .

youth:ExternalViewpoint a owl:Class,
        owl:NamedIndividual ;
    rdfs:comment "Represents the way in which subcultures are viewed from an external viewpoint. This is influenced by the general public's impressions, stereotypes, and media portrayals that shape the external image of a subculture." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:determines ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( youth:NegativeAttitude youth:NeutralAttitude youth:PositiveAttitude ) ] ],
        persp:Lens ;
    owl:disjointWith youth:InternalViewpoint ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue youth:ExternalViewpoint ;
            owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/description.owl#satisfies> ] .

youth:InternalViewpoint a owl:Class,
        owl:NamedIndividual ;
    rdfs:comment "Represents the self-perception and self-definition of members within a subculture. This includes how individuals within the subculture view themselves and their own identity, as well as how they interpret their place within the subculture. It reflects the internal self-concept and personal narratives that members use to describe their experiences and values." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:determines ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( youth:NegativeAttitude youth:PositiveAttitude ) ] ],
        persp:Lens ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue youth:InternalViewpoint ;
            owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/description.owl#satisfies> ] .

youth:creativity a youth:MoralValue .

youth:NeutralAttitude a youth:NeutralAttitude,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:comment "Attitude in this context refers to the evaluative stance or disposition that is held toward a youth subculture, whether by its members or by outsiders. Depending on whether a particular attitude aligns more closely with the values or stereotypes associated with a subculture, it will take on a positive, negative, or neutral connotation." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:expressedVia ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( youth:MoralValue youth:Stereotype ) ] ],
        persp:Attitude ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue youth:NeutralAttitude ;
            owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/description.owl#satisfies> ] .

youth:NegativeAttitude a youth:NegativeAttitude,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:comment "Attitude in this context refers to the evaluative stance or disposition that is held toward a youth subculture, whether by its members or by outsiders. Depending on whether a particular attitude aligns more closely with the values or stereotypes associated with a subculture, it will take on a positive, negative, or neutral connotation." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:expressedVia ;
            owl:someValuesFrom youth:Stereotype ],
        persp:Attitude ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue youth:NegativeAttitude ;
            owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/description.owl#satisfies> ] .

youth:PositiveAttitude a youth:PositiveAttitude,
        owl:Class,
        owl:NamedIndividual ;
    rdfs:comment "Attitude in this context refers to the evaluative stance or disposition that is held toward a youth subculture, whether by its members or by outsiders. Depending on whether a particular attitude aligns more closely with the values or stereotypes associated with a subculture, it will take on a positive, negative, or neutral connotation." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:expressedVia ;
            owl:someValuesFrom youth:MoralValue ],
        persp:Attitude ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue youth:PositiveAttitude ;
            owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/description.owl#satisfies> ] .

youth:FashionStyle a owl:Class ;
    rdfs:comment "Represents the style of clothing and aesthetics associated with a subculture." ;
    rdfs:subClassOf youth:Style ;
    owl:disjointWith youth:MusicGenre .

youth:Influence a owl:Class ;
    rdfs:comment """Represents the factors that shape and affect both external and internal viewpoint. This can include a variety of influences such as media portrayals, social institutions - like family, work, school, home -, personal experiences, education level, and cultural trends, all of which contribute to the evolving perceptions of subcultures by both insiders and outsiders. Hallâ€™s Encoding/Decoding theory is integrated into this analysis, allowing us to deconstruct the role of television as a primary source of information on the subject.\r
\r
See bibliography:\r
Hall, Stuart. "Encoding/Decoding in the Television Discourse." University of Birmingham: Centre for Contemporary Cultural Studies, 1973.\r
Moore, Ryan. "Alternative to What? Subcultural Capital and the Commercialization of a Music Scene." Deviant Behavior 26, no. 3 (2005): 229-252.""" ;
    rdfs:subClassOf [ a owl:Class ;
            owl:unionOf ( youth:NeutralAttitude youth:PositiveAttitude [ a owl:Restriction ;
                        owl:onProperty youth:influences ;
                        owl:someValuesFrom youth:NegativeAttitude ] ) ],
        persp:Background ;
    owl:disjointWith youth:PerspectiveShift .

youth:MusicGenre a owl:Class ;
    rdfs:comment "Represents the type of music associated with a subculture." ;
    rdfs:subClassOf youth:Style .

youth:Ritual a owl:Class ;
    rdfs:comment "Represents the activities that usually characterize a specific youth subculture." ;
    rdfs:subClassOf youth:Style .

youth:Participant a owl:Class ;
    rdfs:comment "Members within the youth subcultures." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:hasViewpoint ;
            owl:someValuesFrom youth:InternalViewpoint ],
        [ a owl:Restriction ;
            owl:onProperty youth:participatesIn ;
            owl:someValuesFrom youth:YouthSubculture ],
        [ a owl:Restriction ;
            owl:onProperty youth:hasAttitude ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( youth:NegativeAttitude youth:NeutralAttitude youth:PositiveAttitude ) ] ],
        <http://etna.istc.cnr.it/framester2/data/framestercore/People> .

youth:YouthSubculture a owl:Class ;
    rdfs:comment """A group of people within a cultural society that represents a specific community formed by younger generations, characterized by distinct values, practices, and identities that differentiate them from the dominant culture.\r
\r
See bibliography:\r
Cohen, Albert K. Delinquent Boys: The Culture of the Gang. New York: Free Press, 1955.\r
Cohen, Phil. "Subcultural Conflict and Working-Class Community." In CCCS Selected Working Papers, edited by Ann Gray, Jan Campbell, and Mark Erickson, 554-577. London: Routledge, 2007.\r
Williams, Patrick J. "Youth Subcultural Studies: Sociological Traditions and Core Concepts." Sociology Compass 1, no. 2 (2007): 572-593.\r
Widdicombe, Sue. "Autobiography and Change: Rhetoric and Authenticity of 'Gothic' Style." In Repertoires and Readings of Texts in Action, edited by Erica Burman and Ian Parker, 94-113. London: Routledge, 1993.""" ;
    rdfs:seeAlso "https://krke24.gitbook.io/krke-project-2023-2024" ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty youth:hasFashionStyle ;
                        owl:someValuesFrom youth:FashionStyle ] [ a owl:Restriction ;
                        owl:onProperty youth:hasMusicGenre ;
                        owl:someValuesFrom youth:MusicGenre ] [ a owl:Restriction ;
                        owl:onProperty youth:hasRitual ;
                        owl:someValuesFrom youth:Ritual ] ) ],
        [ a owl:Restriction ;
            owl:onProperty youth:hasParticipant ;
            owl:someValuesFrom youth:Participant ],
        [ a owl:Restriction ;
            owl:onProperty youth:locatedIn ;
            owl:someValuesFrom youth:Location ],
        [ a owl:Restriction ;
            owl:onProperty youth:originatedIn ;
            owl:someValuesFrom youth:HistoricalPeriod ],
        [ a owl:Restriction ;
            owl:onProperty youth:triggersStereotype ;
            owl:someValuesFrom youth:Stereotype ],
        persp:Eventuality .

youth:Stereotype a owl:Class ;
    rdfs:comment """Represents common stereotypes associated with subcultures, drawing on M. Cardwell's definition of a stereotype as "a fixed, often simplistic generalization about a particular group or class of people. [...] These stereotypes are often negative and unflattering, and may underly prejudice and discrimination". In our ontology, the Stereotype class encapsulates the generalized beliefs and oversimplified notions frequently attributed to members of specific subcultures. These stereotypes reflect societal perceptions and can shape mainstream discourse, influencing how subcultures are understood and portrayed. By capturing these stereotypes, the class facilitates the analysis of misrepresentations and biases that may skew public perception, offering insights into the disparity between societal views of subcultures and their actual characteristics and values.\r
\r
See also:\r
Cardwell, Mike, Dictionary of Psychology, Chicago Fitzroy Dearborn, 1999: 292.""" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:isExpressedVia ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( youth:NegativeAttitude youth:NeutralAttitude youth:PositiveAttitude ) ] ] .

youth:MoralValue a owl:Class ;
    rdfs:comment "Represents the fundamental principles and beliefs that underpin a subculture, guiding the behavior, attitudes, and norms of its members." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:isExpressedVia ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( youth:NegativeAttitude youth:NeutralAttitude youth:PositiveAttitude ) ] ] ;
    owl:disjointWith youth:Ritual,
        youth:Stereotype .

youth:PerspectiveShift a owl:Class ;
    rdfs:comment "Represents the interpretations or portrayals of a youth subculture following the evolution or transformation of perspectives. This class captures shifts in how subcultures are viewed, whether by the public or by their own members. It includes changes in societal attitudes, evolving stereotypes, or shifts in self-perception within the subculture, highlighting how perspectives can develop and alter across different historical or cultural contexts." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:isCharacterisedBy ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( youth:ExternalViewpoint youth:InternalViewpoint ) ] ],
        persp:Cut .

[] a owl:Axiom ;
    rdfs:comment "Values and stereotypes have different implications." ;
    owl:annotatedProperty owl:propertyDisjointWith ;
    owl:annotatedSource youth:hasMoralValue ;
    owl:annotatedTarget youth:triggersStereotype .

[] a owl:Axiom ;
    rdfs:comment "A subculture typically originates in one historical period, so this is a functional property." ;
    owl:annotatedProperty rdfs:comment ;
    owl:annotatedSource youth:originatedIn ;
    owl:annotatedTarget "Connects the youth subculture to its historical period, representing the time frame during which they have developed or undergone significant changes." .

