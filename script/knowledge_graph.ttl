        youth:party-goers .

youth:rebellion a youth:MoralValue .

youth:triggersStereotype a owl:ObjectProperty ;
    rdfs:comment "Links a stereotype to the subcultures that it affects or challenges. This property identifies how certain stereotypes are associated with subcultures and how these stereotypes may influence or be related to the subcultureâ€™s public image. It highlights the stereotypes that subcultures confront or embody." ;
    rdfs:domain youth:YouthSubculture ;
    rdfs:range youth:Stereotype .

youth:environmentalism a youth:MoralValue .

youth:relaxation a youth:MoralValue .

<http://etna.istc.cnr.it/framester2/data/framestercore/People> a owl:Class ;
    rdfs:comment "http://etna.istc.cnr.it/framester2/data/framestercore/" ;
    rdfs:isDefinedBy "This pattern is applied to model the involvement of people within subcultures, distinguishing between participants and non-participants." ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:hasValue persp:Conceptualiser ;
            owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/description.owl#satisfies> ] .

youth:NotParticipant a owl:Class ;
    rdfs:comment "Members outside youth subcultures." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:hasAttitude ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( youth:NegativeAttitude youth:NeutralAttitude youth:PositiveAttitude ) ] ],
        [ a owl:Restriction ;
            owl:onProperty youth:hasViewpoint ;
            owl:someValuesFrom youth:ExternalViewpoint ],
        <http://etna.istc.cnr.it/framester2/data/framestercore/People> .

