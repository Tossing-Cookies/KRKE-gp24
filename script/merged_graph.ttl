youth:hasEthic a owl:ObjectProperty ;
    rdfs:comment "Links a subculture to its core values." ;
    rdfs:domain youth:YouthSubculture ;
    rdfs:range youth:Ethic ;
    owl:propertyDisjointWith youth:triggersStereotype .

youth:hasFashionStyle a owl:ObjectProperty ;
    rdfs:comment "Links a subculture to its fashion style(s)." ;
    rdfs:domain youth:YouthSubculture ;
    rdfs:range youth:FashionStyle .

youth:hasMusicGenre a owl:ObjectProperty ;
    rdfs:comment "Links a subculture to its associated music genre(s)." ;
    rdfs:domain youth:YouthSubculture ;
    rdfs:range youth:MusicGenre .

youth:hasParticipant a owl:ObjectProperty ;
    rdfs:comment "Links a youth subculture to a person (indicating that the person is a member of the subculture)." ;
    rdfs:domain youth:YouthSubculture ;
    rdfs:range youth:Participant ;
    owl:inverseOf youth:participatesIn .

youth:hasRitual a owl:ObjectProperty ;
    rdfs:domain youth:YouthSubculture ;
    rdfs:range youth:Ritual .

youth:isAttitudeOf a owl:ObjectProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( youth:NegativeAttitude youth:NeutralAttitude youth:PositiveAttitude ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( youth:NotParticipant youth:Participant ) ] .

youth:isCharacterisedBy a owl:ObjectProperty ;
    rdfs:comment "This property links a specific external or internal viewpoint to the perspective shift. It captures how the views of subcultures, both internally (self-perception) and externally (public perception), are affected by shifts in perspective." ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( youth:ExternalViewpoint youth:InternalViewpoint ) ] ;
    rdfs:range youth:PerspectiveShift .

youth:isViewpointOf a owl:ObjectProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( youth:ExternalViewpoint youth:InternalViewpoint ) ] ;
    rdfs:range <http://etna.istc.cnr.it/framester2/data/framestercore/People> .

youth:locatedIn a owl:ObjectProperty ;
    rdfs:comment "Links individuals and subcultures to geographic locations." ;
    rdfs:domain youth:YouthSubculture ;
    rdfs:range youth:Location .

youth:NotParticipant a owl:Class ;
    rdfs:comment "Members outside youth subcultures." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty youth:hasViewpoint ;
            owl:someValuesFrom youth:ExternalViewpoint ],
        [ a owl:Class ;
            owl:complementOf [ a owl:Restriction ;
                    owl:onProperty youth:participatesIn ;
                    owl:someValuesFrom youth:YouthSubculture ] ],
        [ a owl:Restriction ;
            owl:onProperty youth:hasAttitude ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( youth:NegativeAttitude youth:NeutralAttitude youth:PositiveAttitude ) ] ],
        <http://etna.istc.cnr.it/framester2/data/framestercore/People> .

