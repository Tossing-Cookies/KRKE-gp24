youth:creates a owl:ObjectProperty .

youth:extractedFrom a owl:ObjectProperty .

youth:hasContent a owl:DatatypeProperty ;
    rdfs:comment "Describes the content of instances from different classes." ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( youth:Ethic youth:FashionStyle youth:HistoricalPeriod youth:Influence youth:MusicGenre youth:PerspectiveShift youth:Ritual youth:Stereotype ) ] ;
    rdfs:range rdfs:Literal .

youth:hasName a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:comment "Indicates the name of a person." ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( youth:NotParticipant youth:Participant ) ] ;
    rdfs:range rdfs:Literal .

youth:holds a owl:ObjectProperty .

youth:influencedBy a owl:ObjectProperty ;
    rdfs:comment "Connects viewpoints to the factors that influence them. This property shows how external and internal viepoints are affected by various influencing factors such as media representations, societal attitudes, and personal experiences. It models the relationship between perspectives and the elements that shape them." ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( youth:NegativeAttitude youth:NeutralAttitude youth:PositiveAttitude ) ] ;
    rdfs:range youth:Influence ;
    owl:inverseOf youth:influences .

youth:isSatisfiedBy a owl:ObjectProperty .

youth:isSettingFor a owl:ObjectProperty .

youth:perspectivisedAs a owl:ObjectProperty .

youth:perspectivisedThrough a owl:ObjectProperty .

youth:satisfies a owl:ObjectProperty .

