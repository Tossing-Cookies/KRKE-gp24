<http://www.semanticweb.org/YOUTH> rdf:type owl:Ontology ;
                                    owl:imports <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl> .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/YOUTH#belongsToYouthSubculture
:belongsToYouthSubculture rdf:type owl:ObjectProperty ;
                          rdfs:domain :SubGroup ;
                          rdfs:range :YouthSubculture .


###  http://www.semanticweb.org/YOUTH#comprises
:comprises rdf:type owl:ObjectProperty ;
           rdfs:domain :Ritual ,
                       :Style ;
           rdfs:range :Ritual ,
                      :Style ;
           rdfs:comment "This property links a ritual or style to its constituent components. It represents the idea that rituals include specific events and activities, while styles include specific elements such as slang, music genres, and fashion styles." .


###  http://www.semanticweb.org/YOUTH#hasActivity
:hasActivity rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasRitual ;
             rdfs:domain :YouthSubculture ;
             rdfs:range :Activity .


###  http://www.semanticweb.org/YOUTH#hasEvent
:hasEvent rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasRitual ;
          rdfs:domain :YouthSubculture ;
          rdfs:range :Event .


###  http://www.semanticweb.org/YOUTH#hasSubGroup
:hasSubGroup rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :includesSubculture ;
             rdfs:domain :YouthSubculture ;
             rdfs:range :SubGroup ;
             rdfs:comment "This property links a youth subculture to the subgroups that are part of it. It emphasizes that a youth subculture is made up of various subgroups, each contributing to the overall structure and identity of the subculture." .


###  http://www.semanticweb.org/YOUTH#influencesPerspective
:influencesPerspective rdf:type owl:ObjectProperty ;
                       rdfs:domain :PerspectiveChange ;
                       rdfs:range :ExternalPerspective ,
                                  :InternalPerspective ;
                       rdfs:comment "This property links a perspective change to the specific external or internal perspectives it influences. It captures how shifts in perspective affect the views of subcultures both internally (self-perception) and externally (public perception)." .


###  http://www.semanticweb.org/YOUTH#occurredDuring
:occurredDuring rdf:type owl:ObjectProperty ;
                rdfs:domain :Event ;
                rdfs:range :HistoricalPeriod .


###  http://www.semanticweb.org/YOUTH#reflectsDominantCulture
:reflectsDominantCulture rdf:type owl:ObjectProperty ;
                         rdfs:domain :CulturalSociety ;
                         rdfs:range :DominantCulture ;
                         rdfs:comment "This property links a cultural society to the dominant culture that it reflects. It indicates that the cultural society embodies and manifests the values, norms, and practices of the dominant culture within its structure and functioning." .


###  http://www.semanticweb.org/YOUTH#triggeredByEvent
:triggeredByEvent rdf:type owl:ObjectProperty ;
                  rdfs:comment "This property connects a perspective change to the events that trigger it. It represents how certain events, such as cultural phenomena, media portrayals, or significant societal incidents, can lead to changes in how a subculture is perceived by both insiders and outsiders." .


###  http://www.semanticweb.org/YOUTH/affectsCulturalPerspective
:affectsCulturalPerspective rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :influences ;
                            rdfs:range :ExternalPerspective .


###  http://www.semanticweb.org/YOUTH/affectsIndividualPerspective
:affectsIndividualPerspective rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :influences ;
                              rdfs:range :InternalPerspective .


###  http://www.semanticweb.org/YOUTH/affectsPerspective
:affectsPerspective rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :influences ;
                    rdfs:domain :YouthSubculture ;
                    rdfs:range :Person ;
                    owl:propertyDisjointWith :triggersStereotype ;
                    rdfs:comment "Represents how a subculture influences perspectives of individuals or other subcultures." .


###  http://www.semanticweb.org/YOUTH/engagesIn
:engagesIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range :Ritual ;
           rdfs:comment "Links individuals to activities they typically engage in." .


###  http://www.semanticweb.org/YOUTH/hasExternalPerspective
:hasExternalPerspective rdf:type owl:ObjectProperty ;
                        rdfs:domain :YouthSubculture ;
                        rdfs:range :ExternalPerspective .


###  http://www.semanticweb.org/YOUTH/hasFashionStyle
:hasFashionStyle rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :relatedTo ;
                 rdfs:domain :YouthSubculture ;
                 rdfs:range :FashionStyle .


###  http://www.semanticweb.org/YOUTH/hasHistoricalContext
:hasHistoricalContext rdf:type owl:ObjectProperty ;
                      rdfs:domain :CulturalSociety ;
                      rdfs:range :HistoricalPeriod ;
                      rdfs:comment "Connects a cultural society to its historical periods, representing the time frames during which it has developed or undergone significant changes." .


###  http://www.semanticweb.org/YOUTH/hasInternalPerspective
:hasInternalPerspective rdf:type owl:ObjectProperty ;
                        rdfs:domain :Person ;
                        rdfs:range :InternalPerspective .


###  http://www.semanticweb.org/YOUTH/hasMusicGenre
:hasMusicGenre rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :relatedTo ;
               rdfs:domain :YouthSubculture ;
               rdfs:range :MusicGenre .


###  http://www.semanticweb.org/YOUTH/hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :participatesIn ;
                rdfs:domain :DominantCulture ,
                            :YouthSubculture ;
                rdfs:range :Person ;
                owl:propertyDisjointWith :hasRitual ;
                rdfs:comment "Links a YouthSubculture to a Person (indicating that the person is a member of the subculture)." .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :hasParticipant ;
   owl:annotatedProperty owl:propertyDisjointWith ;
   owl:annotatedTarget :hasRitual ;
   rdfs:comment "Members and rituals are distinct concepts."
 ] .


###  http://www.semanticweb.org/YOUTH/hasRitual
:hasRitual rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :relatedTo ;
           rdfs:domain :YouthSubculture ;
           rdfs:range :Ritual ;
           rdfs:comment "Links a subculture to its typical rituals." .


###  http://www.semanticweb.org/YOUTH/hasSlang
:hasSlang rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :relatedTo ;
          rdfs:domain :YouthSubculture ;
          rdfs:range :Slang .


###  http://www.semanticweb.org/YOUTH/hasValue
:hasValue rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :relatedTo ;
          rdfs:domain :YouthSubculture ;
          rdfs:range :Value ;
          owl:propertyDisjointWith :triggersStereotype ;
          rdfs:comment "Links a subculture to its core values." .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :hasValue ;
   owl:annotatedProperty owl:propertyDisjointWith ;
   owl:annotatedTarget :triggersStereotype ;
   rdfs:comment "Values and stereotypes have different implications."
 ] .


###  http://www.semanticweb.org/YOUTH/includesSubculture
:includesSubculture rdf:type owl:ObjectProperty ;
                    rdfs:domain :CulturalSociety ;
                    rdfs:range :YouthSubculture ;
                    rdfs:comment "Connects a cultural society to the youth subcultures present within it. This property highlights the diverse subcultures that exist within the broader societal context." .


###  http://www.semanticweb.org/YOUTH/influences
:influences rdf:type owl:ObjectProperty ;
            rdfs:domain :DominantCulture ,
                        :ExternalPerspective ,
                        :InternalPerspective ,
                        :YouthSubculture ;
            rdfs:range :DominantCulture ,
                       :ExternalPerspective ,
                       :InternalPerspective ,
                       :YouthSubculture ;
            rdfs:comment "Represents broader influence relationships or influence relationships between subcultures." .


###  http://www.semanticweb.org/YOUTH/locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Ritual ,
                       :YouthSubculture ;
           rdfs:range :Location .


###  http://www.semanticweb.org/YOUTH/originatedIn
:originatedIn rdf:type owl:ObjectProperty ;
              rdfs:domain :YouthSubculture ;
              rdfs:range :HistoricalPeriod ;
              rdfs:comment "Links subcultures to their historical context." .


###  http://www.semanticweb.org/YOUTH/participatesIn
:participatesIn rdf:type owl:ObjectProperty ;
                rdfs:domain :Person ;
                rdfs:range :DominantCulture ,
                           :YouthSubculture ;
                rdfs:comment "Links a Person to a YouthSubculture (indicating that the person belongs to that subculture)." .


###  http://www.semanticweb.org/YOUTH/relatedTo
:relatedTo rdf:type owl:ObjectProperty ;
           rdfs:domain :YouthSubculture ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Media
                                      :Ritual
                                      :Style
                                    )
                      ] ;
           rdfs:comment """General property for linking various elements. 
It links subcultures to relevant media, rituals or styles.""" .


###  http://www.semanticweb.org/YOUTH/relatedToExpressionMedia
:relatedToExpressionMedia rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :relatedTo ;
                          rdfs:domain :YouthSubculture ;
                          rdfs:range :ExpressionMedia ;
                          rdfs:comment "Links subcultures to forms of expression media." .


###  http://www.semanticweb.org/YOUTH/relatedToMainstreamMedia
:relatedToMainstreamMedia rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :relatedTo ;
                          rdfs:domain :DominantCulture ;
                          rdfs:range :MainstreamMedia ;
                          rdfs:comment "Links subcultures to mainstream media representations." .


###  http://www.semanticweb.org/YOUTH/shapedBy
:shapedBy rdf:type owl:ObjectProperty ;
          rdfs:domain :ExternalPerspective ,
                      :InternalPerspective ;
          rdfs:range :PerspectiveInfluence ;
          rdfs:comment "Connects perspectives to influencing factors." .


###  http://www.semanticweb.org/YOUTH/triggersStereotype
:triggersStereotype rdf:type owl:ObjectProperty ;
                    rdfs:domain :YouthSubculture ;
                    rdfs:range :Stereotype .


