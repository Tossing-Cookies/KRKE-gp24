@prefix : <http://www.semanticweb.org/YOUTH/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/YOUTH/> .

<http://www.semanticweb.org/YOUTH> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/YOUTH/affectsCulturalPerspective
:affectsCulturalPerspective rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :influences ;
                            rdfs:range :ExternalPerspective .


###  http://www.semanticweb.org/YOUTH/affectsIndividualPerspective
:affectsIndividualPerspective rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :influences ;
                              rdfs:range :InternalPerspective .


###  http://www.semanticweb.org/YOUTH/affectsPerspective
:affectsPerspective rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :influences ;
                    rdfs:domain :YouthSubculture ;
                    rdfs:range :Person ;
                    owl:propertyDisjointWith :triggersStereotype ;
                    rdfs:comment "Represents how a subculture influences perspectives of individuals or other subcultures." .


###  http://www.semanticweb.org/YOUTH/engagesIn
:engagesIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range :Ritual ;
           rdfs:comment "Links individuals to activities they typically engage in." .


###  http://www.semanticweb.org/YOUTH/hasExternalPerspective
:hasExternalPerspective rdf:type owl:ObjectProperty ;
                        rdfs:domain :YouthSubculture ;
                        rdfs:range :ExternalPerspective .


###  http://www.semanticweb.org/YOUTH/hasFashionStyle
:hasFashionStyle rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :relatedTo ;
                 rdfs:domain :YouthSubculture ;
                 rdfs:range :FashionStyle .


###  http://www.semanticweb.org/YOUTH/hasInternalPerspective
:hasInternalPerspective rdf:type owl:ObjectProperty ;
                        rdfs:domain :Person ;
                        rdfs:range :InternalPerspective .


###  http://www.semanticweb.org/YOUTH/hasMember
:hasMember rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :memberOf ;
           rdfs:domain :YouthSubculture ;
           rdfs:range :Person ;
           owl:propertyDisjointWith :hasRitual ;
           rdfs:comment "Links a YouthSubculture to a Person (indicating that the person is a member of the subculture)." .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :hasMember ;
   owl:annotatedProperty owl:propertyDisjointWith ;
   owl:annotatedTarget :hasRitual ;
   rdfs:comment "Members and rituals are distinct concepts."
 ] .


###  http://www.semanticweb.org/YOUTH/hasMusicGenre
:hasMusicGenre rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :relatedTo ;
               rdfs:domain :YouthSubculture ;
               rdfs:range :MusicGenre .


###  http://www.semanticweb.org/YOUTH/hasRitual
:hasRitual rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :relatedTo ;
           rdfs:domain :YouthSubculture ;
           rdfs:range :Ritual ;
           rdfs:comment "Links a subculture to its typical rituals." .


###  http://www.semanticweb.org/YOUTH/hasValue
:hasValue rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :relatedTo ;
          rdfs:domain :YouthSubculture ;
          rdfs:range :Value ;
          owl:propertyDisjointWith :triggersStereotype ;
          rdfs:comment "Links a subculture to its core values." .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :hasValue ;
   owl:annotatedProperty owl:propertyDisjointWith ;
   owl:annotatedTarget :triggersStereotype ;
   rdfs:comment "Values and stereotypes have different implications."
 ] .


###  http://www.semanticweb.org/YOUTH/influences
:influences rdf:type owl:ObjectProperty ;
            rdfs:domain :ExternalPerspective ,
                        :InternalPerspective ,
                        :YouthSubculture ;
            rdfs:range :ExternalPerspective ,
                       :InternalPerspective ,
                       :YouthSubculture ;
            rdfs:comment "Represents broader influence relationships or influence relationships between subcultures." .


###  http://www.semanticweb.org/YOUTH/locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Person ,
                       :Ritual ,
                       :YouthSubculture ;
           rdfs:range :Location .


###  http://www.semanticweb.org/YOUTH/memberOf
:memberOf rdf:type owl:ObjectProperty ;
          rdfs:comment "Links a Person to a YouthSubculture (indicating that the person belongs to that subculture)." .


###  http://www.semanticweb.org/YOUTH/originatedIn
:originatedIn rdf:type owl:ObjectProperty ;
              rdfs:domain :YouthSubculture ;
              rdfs:range :HistoricalPeriod ;
              rdfs:comment "Links subcultures to their historical context." .


###  http://www.semanticweb.org/YOUTH/participatesIn
:participatesIn rdf:type owl:ObjectProperty ;
                rdfs:domain :Person ;
                rdfs:range :SubGroup ,
                           :YouthSubculture ;
                rdfs:comment "Links individuals to groups they are part of within the subculture." .


###  http://www.semanticweb.org/YOUTH/relatedTo
:relatedTo rdf:type owl:ObjectProperty ;
           rdfs:domain :YouthSubculture ;
           rdfs:range :Media ,
                      :Style ;
           rdfs:comment """General property for linking various elements. 
It links subcultures to relevant media, rituals or styles.""" .


###  http://www.semanticweb.org/YOUTH/relatedToExpressionMedia
:relatedToExpressionMedia rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :relatedTo ;
                          rdfs:comment "Links subcultures to forms of expression media." .


###  http://www.semanticweb.org/YOUTH/relatedToMainstreamMedia
:relatedToMainstreamMedia rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :relatedTo ;
                          rdfs:comment "Links subcultures to mainstream media representations." .


###  http://www.semanticweb.org/YOUTH/shapedBy
:shapedBy rdf:type owl:ObjectProperty ;
          rdfs:domain :ExternalPerspective ,
                      :InternalPerspective ;
          rdfs:range :PerspectiveInfluence ;
          rdfs:comment "Connects perspectives to influencing factors." .


###  http://www.semanticweb.org/YOUTH/triggersStereotype
:triggersStereotype rdf:type owl:ObjectProperty ;
                    rdfs:domain :Stereotype ;
                    rdfs:range :YouthSubculture .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/YOUTH/hasContext
:hasContext rdf:type owl:DatatypeProperty ;
            rdfs:domain :HistoricalPeriod ;
            rdfs:range rdfs:Literal ;
            rdfs:comment "Represents the time when a subculture originated." .


###  http://www.semanticweb.org/YOUTH/hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:domain :Location ,
                     :Media ,
                     :Ritual ;
         rdfs:range rdfs:Literal ;
         rdfs:comment "Indicates the name of the entity." .


###  http://www.semanticweb.org/YOUTH/hasTrend
:hasTrend rdf:type owl:DatatypeProperty ;
          rdfs:domain :YouthSubculture ;
          rdfs:range xsd:string ;
          rdfs:comment "Describes current trends within a subculture." .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/YOUTH#CulturalSociety
:CulturalSociety rdf:type owl:Class .


###  http://www.semanticweb.org/YOUTH#DominantCultural
:DominantCultural rdf:type owl:Class ;
                  rdfs:subClassOf :CulturalSociety .


###  http://www.semanticweb.org/YOUTH/Activity
:Activity rdf:type owl:Class ;
          rdfs:subClassOf :Ritual .


###  http://www.semanticweb.org/YOUTH/Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :Ritual .


###  http://www.semanticweb.org/YOUTH/ExpressionMedia
:ExpressionMedia rdf:type owl:Class ;
                 rdfs:subClassOf :Media .


###  http://www.semanticweb.org/YOUTH/ExternalPerspective
:ExternalPerspective rdf:type owl:Class ;
                     rdfs:subClassOf :Perspective ;
                     rdfs:comment "Represents how subcultures are viewed by outsiders." .


###  http://www.semanticweb.org/YOUTH/FashionStyle
:FashionStyle rdf:type owl:Class ;
              rdfs:subClassOf :Style ;
              owl:disjointWith :MusicGenre ,
                               :Slang .


###  http://www.semanticweb.org/YOUTH/HistoricalPeriod
:HistoricalPeriod rdf:type owl:Class ;
                  rdfs:comment "Represents the historical background or the time frame of a subculture." .


###  http://www.semanticweb.org/YOUTH/InternalPerspective
:InternalPerspective rdf:type owl:Class ;
                     rdfs:subClassOf :Perspective ;
                     rdfs:comment "Represents self-views of subculture members." .


###  http://www.semanticweb.org/YOUTH/Location
:Location rdf:type owl:Class ;
          rdfs:comment "Geographic context of subcultures." .


###  http://www.semanticweb.org/YOUTH/MainstreamMedia
:MainstreamMedia rdf:type owl:Class ;
                 rdfs:subClassOf :Media ;
                 rdfs:comment "Represents mainstream media forms related to subcultures (e.g. blogs, forums, music videos, etc.)" .


###  http://www.semanticweb.org/YOUTH/Media
:Media rdf:type owl:Class ;
       owl:disjointWith :Ritual .


###  http://www.semanticweb.org/YOUTH/MusicGenre
:MusicGenre rdf:type owl:Class ;
            rdfs:subClassOf :Style ;
            owl:disjointWith :Slang .


###  http://www.semanticweb.org/YOUTH/Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf :CulturalSociety ;
        owl:disjointWith :YouthSubculture ;
        rdfs:comment "Individuals within the subcultures." .


###  http://www.semanticweb.org/YOUTH/Perspective
:Perspective rdf:type owl:Class .


###  http://www.semanticweb.org/YOUTH/PerspectiveChange
:PerspectiveChange rdf:type owl:Class ;
                   rdfs:subClassOf :Perspective ;
                   rdfs:comment "Represents the evolution of perspectives (e.g. shift in public perception of Emo from 'depressing' to 'creative')." .


###  http://www.semanticweb.org/YOUTH/PerspectiveInfluence
:PerspectiveInfluence rdf:type owl:Class ;
                      rdfs:subClassOf :Perspective ;
                      rdfs:comment "Factors influencing perspectives (e.g. media portrayal, societal attitudes)." .


###  http://www.semanticweb.org/YOUTH/Ritual
:Ritual rdf:type owl:Class ;
        owl:disjointWith :Value ;
        rdfs:comment "The rituals or events that characterize a specific subculture." .


###  http://www.semanticweb.org/YOUTH/Slang
:Slang rdf:type owl:Class ;
       rdfs:subClassOf :Style .


###  http://www.semanticweb.org/YOUTH/Stereotype
:Stereotype rdf:type owl:Class ;
            owl:disjointWith :Value ;
            rdfs:comment "Common stereotypes associated with subcultures." .


###  http://www.semanticweb.org/YOUTH/Style
:Style rdf:type owl:Class ;
       owl:disjointWith :YouthSubculture ;
       rdfs:comment "Represents the overall style associated with a subculture, which can be further divided into music, fashion, and slang." .


###  http://www.semanticweb.org/YOUTH/SubGroup
:SubGroup rdf:type owl:Class ;
          rdfs:subClassOf :YouthSubculture ;
          rdfs:comment "Represents communities or groups within a subculture." .


###  http://www.semanticweb.org/YOUTH/Value
:Value rdf:type owl:Class ;
       rdfs:comment "Represents the core values or principles of a subculture." .


###  http://www.semanticweb.org/YOUTH/YouthSubculture
:YouthSubculture rdf:type owl:Class ;
                 rdfs:subClassOf :CulturalSociety ;
                 rdfs:comment "Represents the subculture itself." .


