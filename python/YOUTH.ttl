#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/YOUTH/Activity
:Activity rdf:type owl:Class ;
          rdfs:subClassOf :Ritual .


###  http://www.semanticweb.org/YOUTH/CulturalSociety
:CulturalSociety rdf:type owl:Class ;
                 rdfs:comment "Represents the overarching social structure that encompasses various cultural and subcultural entities." .


###  http://www.semanticweb.org/YOUTH/DominantCulture
:DominantCulture rdf:type owl:Class ;
                 rdfs:subClassOf :CulturalSociety ;
                 rdfs:comment "Represents the prevailing cultural norms and values that are widely accepted and practiced within a society." .


###  http://www.semanticweb.org/YOUTH/Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :Ritual .


###  http://www.semanticweb.org/YOUTH/ExpressionMedia
:ExpressionMedia rdf:type owl:Class ;
                 rdfs:subClassOf :Media .


###  http://www.semanticweb.org/YOUTH/ExternalPerspective
:ExternalPerspective rdf:type owl:Class ;
                     rdfs:subClassOf :Perspective ;
                     rdfs:comment "Represents how subcultures are viewed by outsiders." .


###  http://www.semanticweb.org/YOUTH/FashionStyle
:FashionStyle rdf:type owl:Class ;
              rdfs:subClassOf :Style ;
              owl:disjointWith :MusicGenre ,
                               :Slang .


###  http://www.semanticweb.org/YOUTH/HistoricalPeriod
:HistoricalPeriod rdf:type owl:Class ;
                  rdfs:comment "Represents the historical background or the time frame during which a subculture emerged, developed, or experienced significant events." .


###  http://www.semanticweb.org/YOUTH/InternalPerspective
:InternalPerspective rdf:type owl:Class ;
                     rdfs:subClassOf :Perspective ;
                     rdfs:comment "Represents self-views of subculture members." .


###  http://www.semanticweb.org/YOUTH/Location
:Location rdf:type owl:Class ;
          rdfs:comment "Represents the geographic context or physical setting where a subculture originates, thrives, or is predominantly practiced. This includes specific regions, cities, or areas that are significant to the subculture's identity and activities." .


###  http://www.semanticweb.org/YOUTH/MainstreamMedia
:MainstreamMedia rdf:type owl:Class ;
                 rdfs:subClassOf :Media ;
                 rdfs:comment "Represents mainstream media forms related to subcultures (e.g. blogs, forums, music videos, etc.)" .


###  http://www.semanticweb.org/YOUTH/Media
:Media rdf:type owl:Class ;
       owl:disjointWith :Ritual .


###  http://www.semanticweb.org/YOUTH/MusicGenre
:MusicGenre rdf:type owl:Class ;
            rdfs:subClassOf :Style ;
            owl:disjointWith :Slang .


###  http://www.semanticweb.org/YOUTH/Person
:Person rdf:type owl:Class ;
        owl:disjointWith :YouthSubculture ;
        rdfs:comment "Individuals within the subcultures." .


###  http://www.semanticweb.org/YOUTH/Perspective
:Perspective rdf:type owl:Class .


###  http://www.semanticweb.org/YOUTH/PerspectiveChange
:PerspectiveChange rdf:type owl:Class ;
                   rdfs:subClassOf :Perspective ;
                   rdfs:comment "Represents the evolution of perspectives (e.g. shift in public perception of Emo from 'depressing' to 'creative')." .


###  http://www.semanticweb.org/YOUTH/PerspectiveInfluence
:PerspectiveInfluence rdf:type owl:Class ;
                      rdfs:subClassOf :Perspective ;
                      rdfs:comment "Factors influencing perspectives (e.g. media portrayal, societal attitudes)." .


###  http://www.semanticweb.org/YOUTH/Ritual
:Ritual rdf:type owl:Class ;
        owl:disjointWith :Value ;
        rdfs:comment "The rituals or events that characterize a specific subculture." .


###  http://www.semanticweb.org/YOUTH/Slang
:Slang rdf:type owl:Class ;
       rdfs:subClassOf :Style .


###  http://www.semanticweb.org/YOUTH/Stereotype
:Stereotype rdf:type owl:Class ;
            rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Attitude> ;
            owl:disjointWith :Value ;
            rdfs:comment "Common stereotypes associated with subcultures." .


###  http://www.semanticweb.org/YOUTH/Style
:Style rdf:type owl:Class ;
       owl:disjointWith :YouthSubculture ;
       rdfs:comment "Represents the overall style associated with a subculture, which can be further divided into music, fashion, and slang." .


###  http://www.semanticweb.org/YOUTH/SubGroup
:SubGroup rdf:type owl:Class ;
          rdfs:subClassOf :YouthSubculture ;
          rdfs:comment "Represents smaller groups or factions within a youth subculture, contributing to the diversity within the broader subculture." .


###  http://www.semanticweb.org/YOUTH/Value
:Value rdf:type owl:Class ;
       rdfs:comment "Represents the fundamental principles and beliefs that underpin a subculture, guiding the behavior, attitudes, and norms of its members." .


###  http://www.semanticweb.org/YOUTH/YouthSubculture
:YouthSubculture rdf:type owl:Class ;
                 rdfs:subClassOf :CulturalSociety ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasParticipant ;
                                   owl:someValuesFrom :Person
                                 ] ;
                 rdfs:comment "Represents specific subcultures formed by younger generations, characterized by distinct values, practices, and identities that differentiate them from the dominant culture." .


